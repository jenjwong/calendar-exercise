{
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
      "react/jsx-no-bind":0,
      "semi":1,
      "no-console":1,
      "react/jsx-indent":1,
      "quotes": 1,
      "no-unused-vars": 1,
      "space-before-blocks": 1,
      "no-multiple-empty-lines": 1,
      "newline-after-var": 1,
      "jsx-a11y/no-static-element-interactions": 0,
      "arrow-spacing": 1,
      "jsx-a11y/click-events-have-key-events": 0,
      "jsx-a11y/onclick-has-role": 1,
      "react/jsx-space-before-closing": 1,
      "no-inline-comments": 1,
      "no-empty": 1,
      "react/self-closing-comp": 1,
      "react/jsx-handler-names": 1,
      "jsx-a11y/onclick-has-focus": 1,
      "react/no-unknown-property": 1,
      "arrow-body-style": 1,
      "consistent-return": 1,
      "array-callback-return":1,
      "indent": 1,
      "key-spacing": 1,
      "no-global-assign": 1,
      "no-return-assign": 0,
      "no-param-reassign": 1,
      "no-shadow": 1,
      "space-infix-ops": 0,
      "prefer-template": 1,
      "no-unreachable": 1,
      "no-undef": 1,
      "guard-for-in":1,
      "vars-on-top": 1,
      "no-var": 1
  },
    "extends": "eventbrite-react"
}
